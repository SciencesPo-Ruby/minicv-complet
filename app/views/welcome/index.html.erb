<div class="wrapper">
    <div class="sidebar-wrapper">
        <div class="profile-container">
            <img class="profile img-thumbnail" width="150" src="maphoto.jpeg" alt="" />
            <h1 class="name">Pr√©nom NOM</h1>
            <h3 class="tagline">Full Stack Developer</h3>
        </div><!--//profile-container-->

        <div class="contact-container container-block">
            <ul class="list-unstyled contact-list">
                <li class="email"><i class="fa fa-envelope"></i><a href="mailto:maxime.alayeddine@sciencespo.fr">M'envoyer un email</a></li>
                <li class="phone"><i class="fa fa-phone"></i><a href="tel:0123 456 789">0123 456 789</a></li>
                <li class="website"><i class="fa fa-globe"></i><a href="http://themes.3rdwavemedia.com/website-templates/free-responsive-website-template-for-developers/" target="_blank">portfoliosite.com</a></li>
                <li class="linkedin"><i class="fa fa-linkedin"></i><a href="https://www.google.fr" target="_blank">AFFICHAGE</a></li>
                <li class="github"><i class="fa fa-github"></i><a href="#" target="_blank">github.com/username</a></li>
            </ul>
        </div><!--//contact-container-->
        <div class="education-container container-block">
            <h2 class="container-block-title">Education</h2>
            <% @educations.each do |education| %>
            <div class="item">
                <h4 class="degree"><%= education.nom %></h4>
                <h5 class="meta"><%= education.ecole %></h5>
                <div class="time"><%= education.date_debut %> - <%= if education.date_fin.nil? then "En cours d'obtention" else education.date_fin end %></div>
            </div><!--//item-->
            <% end %>
        </div><!--//education-container-->

        <div class="languages-container container-block">
            <h2 class="container-block-title">Languages</h2>
            <ul class="list-unstyled interests-list">
                <% @languages.each do |language| %>
                    <li><%= language.nom.upcase %> <span class="lang-desc">(<%= language.niveau %>)</span></li>
                <% end %>
            </ul>
        </div><!--//interests-->

        <div class="interests-container container-block">
            <h2 class="container-block-title">Interests</h2>
            <ul class="list-unstyled interests-list">
                <% @interests.each do |interest| %>
                    <li><%= interest.nom %></li>
                <% end %>
            </ul>
        </div><!--//interests-->

    </div><!--//sidebar-wrapper-->

    <div class="main-wrapper" style="height:1800px">

        <section class="section summary-section">
            <h2 class="section-title"><i class="fa fa-user"></i>Career Profile</h2>
            <div class="summary">
                <p>Summarise your career here lorem ipsum dolor sit amet, consectetuer adipiscing elit. You can <a href="http://themes.3rdwavemedia.com/website-templates/orbit-free-resume-cv-template-for-developers/" target="_blank">download this free resume/CV template here</a>. Aenean commodo ligula eget dolor aenean massa. Cum sociis natoque penatibus et magnis dis parturient montes, nascetur ridiculus mus. Donec quam felis, ultricies nec, pellentesque eu.</p>
            </div><!--//summary-->
        </section><!--//section-->

        <section class="section experiences-section">
            <h2 class="section-title"><i class="fa fa-briefcase"></i>Experiences</h2>

          <% @experiences.each do |experience| %>
            <div class="item">
                <div class="meta">
                    <div class="upper-row">
                        <h3 class="job-title"><%= experience.titre %></h3>
                        <div class="time"><%= experience.date_debut %> - <%= experience.date_fin %></div>
                    </div><!--//upper-row-->
                    <div class="company"><%= experience.entreprise %>, <%= experience.lieu %></div>
                </div><!--//meta-->
                <div class="details">
                  <p><%= experience.description %></p>
                </div><!--//details-->
            </div><!--//item-->
          <% end %>

        </section><!--//section-->

        <section class="section projects-section">
            <h2 class="section-title"><i class="fa fa-archive"></i>Projects</h2>
            <% @projects.each do |project| %>
            <div class="item">
                <span class="project-title"><a href="<%= project.lien %>" target="_blank"><%= project.nom %></a></span> -
                <span class="project-tagline"><%= project.description %></span>
            </div><!--//item-->
            <% end %>
        </section><!--//section-->

        <section class="skills-section section">
            <h2 class="section-title"><i class="fa fa-rocket"></i>Skills &amp; Proficiency</h2>
            <div class="skillset">
                <% @skills.each do |skill| %>
                <div class="item">
                    <h3 class="level-title"><%= skill.nom %></h3>
                    <div class="level-bar">
                        <div class="level-bar-inner" data-level="<%= skill.capacite %>%">
                        </div>
                    </div><!--//level-bar-->
                </div><!--//item-->
                <% end %>


            </div>
        </section><!--//skills-section-->

    </div><!--//main-body-->
</div>

